			<script>
			//將Query String的值帶入
			window.onload = function(){
				//Hide Error Code & Symptom
				document.getElementById('wpforms-15952-field_8-container').style.display="none";
				document.getElementById('wpforms-15952-field_4-container').style.display="none";
				document.getElementById('wpforms-15952-field_5-container').style.display="none";
					var getUrlString = location.href;
					var url = new URL(getUrlString);
					//url.searchParams.get('id');
					//Model

					if (url.searchParams.get('model') != '' && url.searchParams.get('model') != null ) {

						//顯示Error Code及Symptom
						document.getElementById('wpforms-15952-field_8-container').style.display="block";
						document.getElementById('wpforms-15952-field_4-container').style.display="block";
						document.getElementById('wpforms-15952-field_5-container').style.display="block";
						//
						var model = document.getElementById("wpforms-15952-field_1");

						for(var i = 0; i < model.options.length; i++) {


							if(model.options[i].innerText == url.searchParams.get('model') ) {

								model.value = model.options[i].value;
								removeOptions(document.getElementById('wpforms-15952-field_4'));
								removeOptions(document.getElementById('wpforms-15952-field_5'));
								getErrList(model.value);
								break;
							}

						}

					}



				}
			
			 var model = document.getElementById('wpforms-15952-field_1');
			 model.addEventListener('change',disableSn,false);
			 //Disable SN欄位及取回error code清單
			 function disableSn(e){
				//顯示Error Code及Symptom
				document.getElementById('wpforms-15952-field_8-container').style.display="block";
				document.getElementById('wpforms-15952-field_4-container').style.display="block";
				document.getElementById('wpforms-15952-field_5-container').style.display="block";
				var select = e.target.value;
				if (select !== 'Not in list') {
					document.getElementById("wpforms-15952-field_3").disabled = true;
					removeOptions(document.getElementById('wpforms-15952-field_4'));
					removeOptions(document.getElementById('wpforms-15952-field_5'));
					getErrList(model.value);
				} else {
					document.getElementById("wpforms-15952-field_3").disabled = false;
				}
			 }

			 var errcode = document.getElementById('wpforms-15952-field_4');
			errcode.addEventListener('change',resetSymptom,false);
			function resetSymptom(e){
				document.getElementById("wpforms-15952-field_5").selectedIndex = 0;
			}
			var symptom = document.getElementById('wpforms-15952-field_5');
			symptom.addEventListener('change',resetErrorCode,false);
			function resetErrorCode(e){
				document.getElementById("wpforms-15952-field_4").selectedIndex = 0;
			}
			
			//SN取回model後更新Error Code List
			var sn = document.getElementById('wpforms-15952-field_3');
			//var symptom = document.getElementById('wpforms-16609-field_20');
			sn.addEventListener('input',getModelName,false);
			//sn.addEventListener('paste',getModelName,false);
			function getModelName(e){
				snv = document.getElementById('wpforms-15952-field_3').value;
				//console.log(snv.length);
				if (snv.length === 16) {
					//顯示Error Code及Symptom
					document.getElementById('wpforms-15952-field_8-container').style.display="block";
					document.getElementById('wpforms-15952-field_4-container').style.display="block";
					document.getElementById('wpforms-15952-field_5-container').style.display="block";
					removeOptions(document.getElementById('wpforms-15952-field_4'));
					removeOptions(document.getElementById('wpforms-15952-field_5'));
					var url = 'https://www.avision.com/en/wp-json/avcust/v1/fae/getmodel/';
					url = url.concat(snv);
					// 發送 Ajax 查詢請求並處理
					var request = new XMLHttpRequest();
					request.open('GET', url);
					request.send();
					
					request.onreadystatechange = function() {
						// 伺服器請求完成
						if (request.readyState === 4) {
							// 伺服器回應成功
							if (request.status === 200) {
								
								var type = request.getResponseHeader('Content-Type');   // 取得回應類型
								//alert(type.indexOf('application/json'));
								// 判斷回應類型，這裡使用 JSON
								if (type.indexOf('application/json') === 0) {               
									var data = JSON.parse(request.responseText);
									if (data['status']==='Success') {
										//console.log(data);
										//console.log(data['result']['type']);
										//document.getElementById('wpforms-15952-field_3').value = data['result']['type'];
										//alert("The model you selected is " + data[0]['type']);
										//document.getElementById("wpforms-15952-field_1").value = 'Not in list';
										document.getElementById("wpforms-15952-field_1").selectedIndex = 0;
										getErrList(data['result']['type']);
									} else {
										//alert("No fetched item.");
										document.getElementById('wpforms-15952-field_3').value = "";
									}
								
								}
								//alert('成功' + request.status);
								
							} else {
								alert('發生錯誤: ' + request.status);
							}
							
						}
					}
				}
			}
			
			//以model取回Error codee及Symptom清單
			function getErrList(model){
				
				var url = 'https://www.avision.com/en/wp-json/avcust/v1/fae/geterrlist/';
				url = url.concat(model);
				// 發送 Ajax 查詢請求並處理
				var request = new XMLHttpRequest();
				request.open('GET', url);
				request.send();
				
				request.onreadystatechange = function() {
					// 伺服器請求完成
					if (request.readyState === 4) {
						// 伺服器回應成功
						if (request.status === 200) {
							
							var type = request.getResponseHeader('Content-Type');   // 取得回應類型
							//alert(type.indexOf('application/json'));
							// 判斷回應類型，這裡使用 JSON
							if (type.indexOf('application/json') === 0) {               
								var data = JSON.parse(request.responseText);
								if (data['status']==='Success') {
									console.log(data);
									if (data['error_code'][0].length > 0) {
										listerr = document.getElementById('wpforms-15952-field_4');
										//移除
										listerr.options.remove(1);
										//加入Error Code的options
										var i;
										for (i = 0; i < data['error_code'][0].length; i++) {
											
											var erritem = new Option(data['error_code'][0][i],data['error_code'][0][i]);
											//var erritem = new Option('GGYY',0);
											listerr.options.add(erritem);
										}
									}
									if (data['symptom'][0].length > 0) {
										//加入Symptom的options
										listsymptom = document.getElementById('wpforms-15952-field_5');
										listsymptom.options.remove(1);
										//加入Error Code的options
										var i;
										for (i = 0; i < data['symptom'][0].length; i++) {
											
											var erritem = new Option(data['symptom'][0][i],data['symptom'][0][i]);
											//var erritem = new Option('GGYY',0);
											listsymptom.options.add(erritem);
										}
									}
								} else {
									//alert("No fetched item.");
									//document.getElementById('wpforms-15952-field_3').value = "";
									removeOptions(document.getElementById('wpforms-15952-field_4'));
								}
							
							}
							//alert('成功' + request.status);
							
						} else {
							alert('ERROR! Refresh page ant try again.' + request.status);
						}
						
					}
				}
			
			}
			
			function removeOptions(selectElement) {
			   var i, L = selectElement.options.length - 1;
			   for(i = L; i >= 0; i--) {
				  selectElement.remove(i);
				}
				var item = new Option('Select',0);
				//var erritem = new Option('GGYY',0);
				selectElement.options.add(item);
			}
			</script>
		
